{
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "memory-cache": "^0.2.0",
    "winston": "^3.11.0",
    "express": "^4.19.2",
    "nodemon": "^2.0.8"
  },
  "devDependencies": {
    "@neondatabase/serverless": "^0.6.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/memory-cache": "^0.2.5",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "nodemon": "^2.0.8",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "nodemonConfig": {
    "ext": "ts,json",
    "exec": "npm run build && ts-node src/index.ts",
    "delay": "2"
  },
  "scripts": {
    "start": "nodemon --ignore ts-node src/**/*.ts || exit 0",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "format:check": "prettier --check 'src/**/*.{js,ts,json,md}'",
    "vercel-start": "node index.js"
  }
}